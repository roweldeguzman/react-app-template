{
	"compilerOptions": {
		"target": "es6",
    "module": "commonjs",
		"baseUrl": "./src",
		"paths": {
			"@core/*": ["./src/core"],
			"@hooks/*": ["./src/hooks"],
			"@httpRequests/*": ["./src/httpRequests"],
      "@resources/*": ["./src/resources"],
      "@global/*": ["./src/global"]
		}
	},
	"include": ["./src"],
	"exclude": ["node_modules"]
}